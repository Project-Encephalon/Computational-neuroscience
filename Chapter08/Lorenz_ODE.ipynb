      "source": [
        "#ode function for lorenz system\n",
        "\n",
        "import numpy as np\n",
        "udot = np.zeros((3, 1), dtype = \"float64\") + 0.5\n",
        "\n",
        "def lorenz_ode(t,u,flag,a,b,c):\n",
        "  \n",
        "  udot[1]=a*(u[2]-u[1])\n",
        "  udot[2]=u[1]*(b-u[3]-u[2])\n",
        "  udot[3]=u[1]*u[2]-c*u[3]\n",
        "  udot=np.transpose(udot)\n",
        "  return udot"
      ],
